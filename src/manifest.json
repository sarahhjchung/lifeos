{
  "name": "LifeOS",
  "version": "0.1",
  "description": "A Chrome extension for facilitating productivity with Pomodoro timing and audio integrations",
  "permissions": [
    "activeTab",
    "identity",
    "alarms"
  ],
  "background": {
    "scripts": ["background.js"],
    "persistent": false
  },
  "browser_action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "assets/LifeOS 16x16.png",
      "32": "assets/LifeOS 32x32.png",
      "48": "assets/LifeOS 48x48.png",
      "128": "assets/LifeOS 128x128.png"
    }
  },
  "manifest_version": 2
}
